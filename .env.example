# Traffic Heatmap - Environment Configuration


# ===========================================
# RTSP Camera Configuration
# ===========================================

RTSP_URL=rtsp://username:password@camera_ip:554/stream1
RTSP_USERNAME=admin
RTSP_PASSWORD=your_password
CAMERA_IP=192.168.1.100
RTSP_PORT=554

# ===========================================
# YOLOv8 Model Configuration
# ===========================================

YOLO_MODEL=yolov8n.pt
CONFIDENCE_THRESHOLD=0.5

# ===========================================
# Video/Stream Settings
# ===========================================
VIDEO_FILE_PATH=./data/test_video.mp4

# Frame processing settings
FRAME_WIDTH=640
FRAME_HEIGHT=480
FPS_LIMIT=30

# ===========================================
# Output Settings
# ===========================================

OUTPUT_DIR=./output
SAVE_DETECTIONS=true
DISPLAY_WINDOW=true

# ===========================================
# Debug Settings
# ===========================================

DEBUG_MODE=false
LOG_LEVEL=INFO
